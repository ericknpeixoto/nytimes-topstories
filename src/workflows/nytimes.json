{
  "job_id": "DATABRICKS_JOBID",
  "new_settings": {
    "name": "New York Times",
    "email_notifications": {
      "no_alert_for_skipped_runs": false
    },
    "webhook_notifications": {},
    "timeout_seconds": 0,
    "schedule": {
      "quartz_cron_expression": "7 05 19 * * ?",
      "timezone_id": "UTC",
      "pause_status": "UNPAUSED"
    },
    "max_concurrent_runs": 1,
    "tasks": [
      {
        "task_key": "Bronze_Ingestion_TopStories",
        "run_if": "ALL_SUCCESS",
        "notebook_task": {
          "notebook_path": "src/bronze/Bronze - Ingestion",
          "base_parameters": {
            "schema_name": "bronze",
            "database_name": "nytimes",
            "table_name": "top_stories",
            "partition_column": "ref_date"
          },
          "source": "GIT"
        },
        "timeout_seconds": 0,
        "email_notifications": {
          "on_success": [
            "DATABRICKS_USER"
          ],
          "on_failure": [
            "DATABRICKS_USER"
          ]
        },
        "notification_settings": {
          "no_alert_for_skipped_runs": false,
          "no_alert_for_canceled_runs": false,
          "alert_on_last_attempt": false
        },
        "webhook_notifications": {}
      },
      {
        "task_key": "Silver_Ingestion_TopStories",
        "depends_on": [
          {
            "task_key": "Bronze_Ingestion_TopStories"
          }
        ],
        "run_if": "ALL_SUCCESS",
        "notebook_task": {
          "notebook_path": "src/silver/full_load/Silver - Ingestion",
          "base_parameters": {
            "database_name": "silver",
            "schema_name": "nytimes",
            "table_name": "top_stories"
          },
          "source": "GIT"
        },
        "timeout_seconds": 0,

        "email_notifications": {
          "on_success": [
            "DATABRICKS_USER"
          ],
          "on_failure": [
            "DATABRICKS_USER"
          ]
        },
        "notification_settings": {
          "no_alert_for_skipped_runs": false,
          "no_alert_for_canceled_runs": false,
          "alert_on_last_attempt": false
        },
        "webhook_notifications": {}
      },
      {
        "task_key": "Silver_Ingestion_TopStories_TEST",
        "depends_on": [
          {
            "task_key": "Silver_Ingestion_TopStories"
          }
        ],
        "run_if": "ALL_SUCCESS",
        "notebook_task": {
          "notebook_path": "src/silver/cdf/Silver - Ingestion",
          "base_parameters": {
            "database_name": "silver",
            "schema_name": "nytimes",
            "table_name": "top_stories"
          },
          "source": "GIT"
        },
        "timeout_seconds": 0,

        "email_notifications": {
          "on_success": [
            "DATABRICKS_USER"
          ],
          "on_failure": [
            "DATABRICKS_USER"
          ]
        },
        "notification_settings": {
          "no_alert_for_skipped_runs": false,
          "no_alert_for_canceled_runs": false,
          "alert_on_last_attempt": false
        },
        "webhook_notifications": {}
      }
    ],
    "git_source": {
      "git_url": "https://github.com/ericknpeixoto/nytimes-topstories",
      "git_provider": "gitHub",
      "git_branch": "main"
    },
    "queue": {
      "enabled": true
    },
    "performance_target": "STANDARD",
    "run_as": {
      "user_name": "DATABRICKS_USER"
    }
  }
}